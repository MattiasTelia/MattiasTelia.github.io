{
  "name": "@telia-ace/websdk-mono",
  "version": "31.0.0",
  "description": "Telia ACE Web SDK monorepo.",
  "author": "Telia Company AB",
  "engines": {
    "node": ">=14.15.0"
  },
  "packageManager": "yarn@3.6.3",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "po": "yarn workspace @telia-ace/cobrowsing-shared run po && yarn workspace @telia-ace/proactive-web run po",
    "chat-api-test": "yarn watch-all & yarn workspace @telia-ace/chat-api-test start",
    "one-widget-plugins-test": "yarn watch-all & yarn workspace @telia-ace/one-widget-plugins-test start",
    "cobrowsing-test": "yarn watch-all & yarn workspace @telia-ace/cobrowsing-test start",
    "proactive-web-test": "yarn watch-all & yarn workspace @telia-ace/proactive-web-test start",
    "web-api-test": "yarn watch-all & yarn workspace @telia-ace/web-api-test start",
    "id-container-test": "yarn watch-all & yarn workspace @telia-ace/websdk-id-container-test start",
    "web-sdk-test": "yarn run web-sdk",
    "web-sdk": "yarn build-all:dev-web-sdk && yarn watch-all & yarn web-sdk:start",
    "web-sdk:start": "yarn workspace @telia-ace/web-sdk start",
    "clean-all:dist": "yarn workspaces foreach -vpA exec 'rm -rf ./dist ./dist-debug ./lib ./lib-esm'",
    "clean-all:modules": "yarn workspaces foreach -vpA exec 'rm -rf ./node_modules'",
    "clean-all:eslintcache": "yarn workspaces foreach -vpA exec 'rm -f ./.eslintcache'",
    "clean-all": "yarn clean-all:dist & yarn clean-all:modules & yarn clean-all:eslintcache",
    "build-all:dev": "yarn workspaces foreach -vptA --jobs 32 --exclude '**/*-test' --exclude '**/websdk-mono' run build:dev",
    "build-all:dev-web-sdk": "yarn workspaces foreach -vptA --jobs 32 --exclude '**/*-test' --exclude '**/websdk-mono' --exclude '**/web-sdk' run build:dev",
    "build-all:prod": "yarn workspaces foreach -vptA --jobs 32 --exclude '**/*-test' --exclude '**/websdk-mono' run build:prod",
    "watch-all": "yarn workspaces foreach -vipA --jobs 32 --exclude '**/*-test' --exclude '**/websdk-mono' --exclude '**/web-sdk' run watch",
    "eslint:report": "yarn workspaces foreach -vpA --jobs 32 --exclude '**/websdk-mono' run eslint:report && eslint \"**/*.js\" --cache",
    "eslint:fix": "yarn workspaces foreach -vpA --exclude '**/websdk-mono' run eslint:fix && eslint \"**/*.js\" --fix",
    "prettier:report": "yarn workspaces foreach -vpA --jobs 32 --exclude '**/websdk-mono' run prettier:report && prettier \"**/*.{js,json,md}\" --check",
    "prettier:fix": "yarn workspaces foreach -vpA --exclude '**/websdk-mono' run prettier:fix && prettier \"**/*.{js,json,md}\" --write",
    "unit-test:report": "jest --silent",
    "unit-test:watch": "jest --watch",
    "browser-test": "yarn workspaces foreach -vpA run browser-test",
    "postinstall": "husky install",
    "dev": "yarn run web-sdk",
    "publish": "yarn lerna publish --no-private",
    "rn": "cd docs/release_notes && pandoc --defaults=release_notes.yaml",
    "ci": "cd docs/configuration_instructions && pandoc --defaults=configuration_instructions.yaml",
    "docs": "yarn rn && yarn ci",
    "null": ""
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "babel-jest": "^29.7.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^3.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-localstorage-mock": "^2.4.26",
    "prettier": "^2.8.8",
    "request": "^2.88.2",
    "ts-jest": "^27.1.5"
  }
}
